{% extends 'admin-v2/layout.html.twig' %}
{% import 'admin-v2/macro.html.twig' as admin_macro %}

{% set menu = 'admin_v2_sms_request_log_list' %}


{% block main %}
  <table id="teacher-table" class="table table-striped table-hover" data-search-form="#user-search-form">
    <thead>
    <tr>
      <th>{{'sms.defence.request.log.coordinate'|trans}}</th>
      <th>{{'sms.defence.request.log.ip'|trans}}</th>
      <th>{{'sms.defence.request.log.mobile'|trans}}</th>
      <th>{{'sms.defence.request.log.is_illegal'|trans}}</th>
      <th>{{'sms.defence.request.log.disable_type'|trans}}</th>
      <th>{{'sms.defence.request.log.created_time'|trans}}</th>
      <th width="12%">{{'sms.defence.request.log.view_record_label'|trans}}</th>
    </tr>
    </thead>
    <tbody>
    {% for smsRequestLog in smsRequestLogs %}
      {% include 'admin-v2/system/sms-defence/sms-request-log/table-tr.html.twig' with {smsRequestLog:smsRequestLog} %}
    {% endfor %}
    </tbody>
  </table>
  {{ admin_macro.paginator(paginator) }}
{% endblock %}