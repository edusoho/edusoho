{% extends 'admin-v2/layout.html.twig' %}
{% import 'admin-v2/macro.html.twig' as admin_macro %}

{% set menu = 'admin_v2_sms_black_ip_list' %}

{% do script(['app/js/admin-v2/system/sms-defence/index.js']) %}

{% block main %}

  <table id="teacher-table" class="table table-striped table-hover" data-search-form="#user-search-form">
    <thead>
    <tr>
      <th>{{'smd.defence.black.ip.list.ip'|trans}}</th>
      <th>{{'smd.defence.black.ip.list.expire_time'|trans}}</th>
      <th>{{'smd.defence.black.ip.list.created_time'|trans}}</th>
      <th width="12%">{{'smd.defence.black.ip.unlock'|trans}}</th>
    </tr>
    </thead>
    <tbody>
    {% for smsBlackIp in smsBlackIpList %}
      {% include 'admin-v2/system/sms-defence/sms-black-ip/table-tr.html.twig' with {smsBlackIp:smsBlackIp} %}
    {% endfor %}
    </tbody>
  </table>
  {{ admin_macro.paginator(paginator) }}
{% endblock %}