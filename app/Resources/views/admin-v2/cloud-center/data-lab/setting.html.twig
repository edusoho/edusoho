{% extends 'admin-v2/layout.html.twig' %}

{% set menu = 'admin_v2_cloud_data_lab_setting' %}
{% set script_controller = 'data-lab/setting' %}

{% block main %}
{% set xapiSetting = setting('xapi') %}
<div class="mtl">
  {{ 'admin.datalab.setting.operation_title'|trans }} 
  <label style="margin-bottom: -3px;" id="xapi-swith" data-disable="{{ path('admin_v2_cloud_data_lab_setting_disable_modal') }}" data-enabled="{{ path('admin_v2_cloud_data_lab_setting_enable_modal') }}" for="switch" class="mls form-switch {% if xapiSetting.enabled|default(0) %}checked{% endif %}">
  </label> 
  <a class="mls" href="http://open.edusoho.com/doc/agreement/privacy-content.html" target="_blank">
    《{{ 'admin.datalab.setting.agreement'|trans }}》
  </a>
</div>
{% endblock %}