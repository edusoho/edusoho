{% extends 'admin-v2/data-statistics/statistics/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/video' %}
{% block analysisBody %}
  <div class="col-md-12">
    {% if tab=="trend" %}
    <div id="line-data"></div>
      <input id="data"  type="hidden" value='{% if data %}{{data}}{% endif %}'>
    {% elseif tab=="detail" %}
      {% include 'admin-v2/data-statistics/statistics/video-view.table.html.twig' %}
    {% endif %}
  </div>
{% endblock %}
