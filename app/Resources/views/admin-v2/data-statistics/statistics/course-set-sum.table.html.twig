<table id="user-table" class="table table-striped table-hover" data-search-form="#user-search-form">
  <thead>
    <tr>
      <th>{{'admin.operation_analysis.course.name'|trans}}</th>
      <th>{{'admin.operation_analysis.course.creator'|trans}}</th>
      <th>{{'site.default.student'|trans}}{{'admin.operation_analysis.course.student_unit'|trans}}</th>
    </tr>
  </thead>
  <tbody>
    {% if courseSetSumDetail %}
      {% for data in courseSetSumDetail %}
        <tr>
          <td> <a href="{{ path('course_show', {id:data.id}) }}" target="_blank"><strong>{{ data.title }}</strong></a><br><span class="text-muted text-sm">{{'admin.operation_analysis.course.category'|trans}}ï¼š{{ categories[data.categoryId].name|default('--') }}</span></td>
          <td>{{ admin_macro.user_link(users[data.creator]) }}<br>{{data.createdTime|date("Y-m-d H:i:s")}}</td>
          <td>{{data.studentNum}}</td>
        </tr>
      {% endfor %}
    {% else %}
      <tr><td colspan="4"><div class="empty">{{'admin.operation_analysis.course.empty'|trans}}</div></td></tr>
    {% endif %}
  </tbody>
</table>
{{ admin_macro.paginator(paginator) }}
