{% extends 'admin/operation-analysis/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/exit-lesson' %}
{% set href="admin_operation_analysis_lesson_exit" %}
{% set menu = 'exit-lesson' %}
{% block analysisBody %}
  <div class="col-md-12">
		{% if tab=="trend" %}
			<div id="line-data"></div>
			<input id="data"  type="hidden" value='{% if data %}{{data}}{% endif %}'>
			<input id="exitLessonStartDate"  type="hidden" value='{% if exitLessonStartDate %}{{exitLessonStartDate}}{% endif %}'>
		{% elseif tab=="detail" %}
			{% include 'admin/operation-analysis/exit-lesson.table.html.twig' %}
		{% endif %}
  </div>
{% endblock %}
