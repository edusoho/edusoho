{% extends 'admin/operation-analysis/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/finished-lesson' %}
{% set href="admin_operation_analysis_task_completed" %}
{% set menu = 'finished-lesson' %}
{% block analysisBody %}
  <div class="col-md-12">
		{% if tab=="trend" %}
			<div id="line-data"></div>
			<input id="data"  type="hidden" value='{% if data %}{{data}}{% endif %}'>
			<input id="completedTaskStartDate"  type="hidden" value='{% if completedTaskStartDate %}{{completedTaskStartDate}}{% endif %}'>
				{% elseif tab=="detail" %}
					{% include 'admin/operation-analysis/completed-task.table.html.twig' %}
				{% endif %}
  </div>
{% endblock %}
