      <table id="user-table" class="table table-striped table-hover" data-search-form="#user-search-form">
          <thead>
            <tr>
              <th>{{'admin.operation_analysis.teach_plan'|trans}}</th>
              <th>{{'admin.operation_analysis.teach_plan.creator'|trans}}</th>
              <th>{% if setting('default.user_name') %}{{'%user_name%'|trans({'%user_name%': setting('default.user_name')|default('site.default.student'|trans)})}}{% endif %}{{'admin.operation_analysis.course.student_unit'|trans}}</th>
            </tr>
          </thead>
          <tbody>
            {% if courseSumDetail %}
              {% for data in courseSumDetail %}
                <tr>
                    <td> <a href="{{ path('course_show', {id:data.id}) }}" target="_blank"><strong>{{ data.title }}</strong></a><br></td>
                     <td>{{ admin_macro.user_link(users[data.creator]) }}<br>{{data.createdTime|date("Y-m-d H:i:s")}}</td>
                     <td>{{data.studentNum}}</td>
                </tr>
              {% endfor %}
            {% else %}
              ã€€<tr><td colspan="4"><div class="empty">{{'admin.operation_analysis.teach_plan.empty'|trans}}</div></td></tr>
            {% endif %}
          </tbody>
      </table>    
      {{ admin_macro.paginator(paginator) }}