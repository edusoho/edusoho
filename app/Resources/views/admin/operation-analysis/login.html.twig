{% extends 'admin/operation-analysis/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/login' %}
{% set href="admin_operation_analysis_login" %}
{% set menu = 'login' %}
{% block analysisBody %}
  <div class="col-md-12">
    {% if tab=="trend" %}
      <div id="line-data"></div>
      <input id="data" type="hidden" value='{% if data %}{{ data }}{% endif %}'>
      <input id="loginStartDate" type="hidden" value='{% if loginStartDate %}{{ loginStartDate }}{% endif %}'>
    {% elseif tab=="detail" %}
      {% include 'admin/operation-analysis/login.table.html.twig' %}
    {% endif %}
  </div>
{% endblock %}
